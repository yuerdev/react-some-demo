{
  "compilerOptions": {
    "paths": {
      "*": [
        "./__types__/*",
        "./*"
      ]
    },
    "jsxImportSource": "@emotion/react",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "@yuer/label-plugin-access-control",
    "jest.config.js",
    "@yuer/babel-demo/babel-demo.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}